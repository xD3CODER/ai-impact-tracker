{
	"name": "ai-impact-tracker",
	"version": "1.0.2",
	"description": "Extension Chrome pour mesurer l'empreinte carbone et hydrique des requÃªtes IA",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "wxt",
		"dev:firefox": "wxt -b firefox",
		"build": "wxt build",
		"build:firefox": "wxt build -b firefox",
		"zip": "wxt zip",
		"zip:firefox": "wxt zip -b firefox",
		"postinstall": "wxt prepare",
		"type-check": "tsc --noEmit",
		"format": "biome format --write .",
		"lint": "biome lint --write .",
		"i18n:validate": "bunx tsx scripts/validate-i18n.ts",
		"prepare": "husky",
		"clean": "rimraf ./.output && rimraf ./.wxt"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"cssnano": "^7.0.7",
		"autoprefixer": "^10.4.21",
		"husky": "^9.1.7",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"rimraf": "^6.0.1",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"wxt": "^0.20.7"
	},
	"dependencies": {
		"@pandacss/dev": "^0.54.0",
		"@pandacss/types": "^0.54.0",
		"@types/react-router-dom": "^5.3.3",
		"@wxt-dev/i18n": "^0.2.4",
		"@wxt-dev/module-react": "^1.1.3",
		"@wxt-dev/storage": "^1.1.1",
		"panda-css": "^1.0.0",
		"preact": "^10.26.9",
		"preact-render-to-string": "^6.5.13",
		"react-router-dom": "^7.6.3",
		"react-shadow": "^20.6.0"
	},
	"engines": {
		"node": ">=18.0.0",
		"bun": ">=1.0.0"
	},
	"keywords": [
		"chrome-extension",
		"ai",
		"carbon-footprint",
		"sustainability",
		"typescript",
		"wxt"
	],
	"author": "Votre Nom",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/votre-username/ai-carbon-tracker.git"
